<%- include partials/header %> 
	<h1>Compose</h1>
	<form action="/compose" method="post">
		<div class="form-group">
			<label for="blogTitle">Title</label>
			<input class="form-control" type="text" name="blogTitle" id="blogTitle">
		</div>
		<div class="form-group">
			<label for="blogPost">Post</label>
			<textarea class="form-control" name="blogPost" id="blogPost" cols="30" rows="8"></textarea>
		</div>
		<button class="btn btn-primary" type="submit" name="publish">Publish</button>
	</form>
<%- include partials/footer %> 
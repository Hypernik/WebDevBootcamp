<% include partials/header %> 
	<h1>Welcome</h1>
	<p> <%= startingContent %>  </p>
	<% posts.forEach(function(post){ %>
		<h1><%= post.title %></h1>
		<p>
			<%= _.truncate(post.body, {'seperator':' ', 'length':100}) %>
			<a href="/posts/<%=post.title%>">Read More</a>
		</p>
		
	<% }); %>
<% include partials/footer %> 